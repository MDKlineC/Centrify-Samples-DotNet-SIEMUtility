<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <system.web.extensions>
    <scripting>
      <webServices>
        <jsonSerialization maxJsonLength="999999999"/>
      </webServices>
    </scripting>
  </system.web.extensions>
  <appSettings>
    <add key="CentrifyEndpointUrl" value="https://tenant.my.centrify.com" />
    <add key="AdminUserName" value="adminuser" />
    <add key="AdminPassword" value="adminpass" />
  </appSettings>
</configuration>